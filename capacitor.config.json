{
  "appId": "com.app.chatter",
  "appName": "Chatter",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": false
    },
    "SplashScreen": {
      "launchShowDuration": 5000,
      "launchAutoHide": true,
      "androidScaleType": "CENTER_CROP",
      "androidSplashResourceName": "splash",
      "splashFullScreen": false,
      "splashImmersive": false
    },
    "android": {
      "allowMixedContent": true
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "server": {
    "url": "http://192.168.10.13:8100"
  }
}
