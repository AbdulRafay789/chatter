{
  "appId": "com.app.chatter",
  "appName": "Chatter",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": false
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "server": {
    "url": "http://192.168.8.100:8100"
  }
}
